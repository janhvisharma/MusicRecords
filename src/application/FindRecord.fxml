<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FindRecordController">
	<children>
		<AnchorPane prefHeight="350.0" prefWidth="400.0">
			<children>
				<Label layoutX="94.0" layoutY="14.0" text="FIND RECORD">
					<font>
						<Font name="System Bold" size="32.0" />
					</font>
				</Label>
				<Label layoutX="41.0" layoutY="79.0" text="NAME :">
					<font>
						<Font size="16.0" />
					</font>
				</Label>
				<Label layoutX="37.0" layoutY="108.0" text="ARTIST : ">
					<font>
						<Font size="16.0" />
					</font>
				</Label>
				<TextField fx:id="name" layoutX="106.0" layoutY="79.0" />
				<TextField fx:id="artist" layoutX="106.0" layoutY="108.0" />
				<Button fx:id="btnSearch" layoutX="284.0" layoutY="81.0" mnemonicParsing="false" onAction="#onSearchClicked" prefHeight="47.0" prefWidth="76.0" text="SEARCH">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Button>
				<TextArea fx:id="txtArea" layoutX="25.0" layoutY="153.0" prefHeight="146.0" prefWidth="350.0">
					<font>
						<Font size="16.0" />
					</font>
				</TextArea>
				<Button fx:id="btnCancel" layoutX="168.0" layoutY="311.0" mnemonicParsing="false" onAction="#onCancelClicked" text="CANCEL">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</children>
</AnchorPane>

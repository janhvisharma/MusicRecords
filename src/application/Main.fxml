<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="500.0"
	xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.MainController">
	<children>
		<AnchorPane prefHeight="400.0" prefWidth="500.0">
			<children>
				<Label layoutX="122.0" layoutY="14.0" text="MUSIC RECORDS">
					<font>
						<Font name="System Bold" size="32.0" />
					</font>
				</Label>
				<Button fx:id="btnAdd" layoutX="146.0" layoutY="88.0"
					mnemonicParsing="false" onAction="#onAddClicked" prefHeight="25.0"
					prefWidth="209.0" text="ADD RECORD">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Button>
				<Button fx:id="btnFind" layoutX="146.0" layoutY="137.0"
					mnemonicParsing="false" onAction="#onFindClicked" prefHeight="25.0"
					prefWidth="209.0" text="FIND RECORD">
					<font>
						<Font name="System Bold" size="13.0" />
					</font>
				</Button>
				<Button fx:id="btnDelete" layoutX="146.0" layoutY="232.0"
					mnemonicParsing="false" onAction="#onDeleteClicked" prefHeight="25.0"
					prefWidth="209.0" text="DELETE RECORDS">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Button>
				<Button fx:id="btnExit" layoutX="146.0" layoutY="280.0"
					mnemonicParsing="false" onAction="#onExitClicked" prefHeight="25.0"
					prefWidth="209.0" text="EXIT">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Button>
				<Button fx:id="btnList" layoutX="146.0" layoutY="185.0"
					mnemonicParsing="false" onAction="#onListClicked" prefHeight="25.0"
					prefWidth="209.0" text="LIST ALL RECORDS">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
